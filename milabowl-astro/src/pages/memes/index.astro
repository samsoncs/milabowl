import Markdown from "../../components/core/Typography/Markdown.astro";
import MarkdownLayout from "../../layouts/MarkdownLayout.astro";

const memeEntries = await getCollection('memes');

const { Content } = await memeEntries[0].render();
---
<MarkdownLayout title="Mila blog">
    <Markdown>
        <Content/>
    </Markdown>
</MarkdownLayout>